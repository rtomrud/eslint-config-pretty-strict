{
  "name": "eslint-config-pretty-strict",
  "version": "1.0.0-rc.16",
  "description": "A strict ESLint config with Prettier as a code formatter",
  "keywords": [
    "config",
    "eslint",
    "format",
    "hint",
    "lint",
    "prettier",
    "pretty",
    "style"
  ],
  "homepage": "https://github.com/rtomrud/eslint-config-pretty-strict#readme",
  "bugs": {
    "url": "https://github.com/rtomrud/eslint-config-pretty-strict/issues"
  },
  "license": "MIT",
  "author": "Artem Rudenko <rtomrud@gmail.com>",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rtomrud/eslint-config-pretty-strict.git"
  },
  "scripts": {
    "pretest": "eslint --print-config index.js | eslint-config-prettier-check",
    "test": "tape index.test.js",
    "lint": "eslint *.js"
  },
  "devDependencies": {
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "1.3.1",
    "prettier": "1.16.4",
    "tape": "4.10.1"
  },
  "peerDependencies": {
    "eslint": "^5.12.0",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "^1.15.3"
  },
  "engines": {
    "node": "6 || 8 || 10 || 11"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "./index.js"
    ],
    "parserOptions": {
      "ecmaVersion": 2015
    },
    "root": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && npm run lint"
    }
  }
}
