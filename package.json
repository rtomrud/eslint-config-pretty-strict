{
  "name": "eslint-config-pretty-strict",
  "version": "1.0.0-rc.2",
  "description": "A strict ESLint config with Prettier as a code formatter",
  "keywords": [
    "check",
    "clean",
    "code",
    "consistent",
    "enforce",
    "eslint",
    "format",
    "hint",
    "lint",
    "policy",
    "prettier",
    "quality",
    "style"
  ],
  "homepage": "https://github.com/rtomrud/eslint-config-pretty-strict#readme",
  "bugs": {
    "url": "https://github.com/rtomrud/eslint-config-pretty-strict/issues"
  },
  "license": "MIT",
  "author": "Artem Rudenko <rtomrud@gmail.com>",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rtomrud/eslint-config-pretty-strict.git"
  },
  "scripts": {
    "pretest": "eslint --print-config index.js | eslint-config-prettier-check",
    "test": "tape index.test.js",
    "posttest": "eslint ."
  },
  "devDependencies": {
    "eslint": "^4.18.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.15.0-beta.16",
    "prettier": "1.10.2",
    "tape": "^4.8.0"
  },
  "peerDependencies": {
    "eslint": "^4.18.0",
    "eslint-plugin-prettier": "^2.6.0",
    "prettier": "^1.10.2"
  },
  "engines": {
    "node": "4 || 6 || 8 || 9"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "./index.js"
    ],
    "parserOptions": {
      "ecmaVersion": 2015
    },
    "root": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
